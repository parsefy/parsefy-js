{
  "name": "parsefy",
  "version": "1.1.0",
  "description": "Official TypeScript SDK for Parsefy - Financial Document Infrastructure for Developers",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/parsefy/parsefy-js.git"
  },
  "homepage": "https://parsefy.io",
  "bugs": {
    "url": "https://github.com/parsefy/parsefy-js/issues"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup && cp dist/index.d.ts dist/index.d.cts",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "dependencies": {
    "zod-to-json-schema": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "zod": "^3.22.0"
  },
  "engines": {
    "node": ">=18"
  }
}
